  <script>
    
      
    $("#dataTable").on("click", ".button-update", function () {
      let id = $(this).data("id");
      let Nama=$(this).data("Nama");
      let NIM=$(this).data("NIM");
      let matkul=$(this).data({});

      $("#edit_modal").modal("show");
      
      $(".id").val(id);
      $(".Nama").val(Nama);
      $(".NIM").val(NIM);
      $(".matkul").val(matkul);

    });
